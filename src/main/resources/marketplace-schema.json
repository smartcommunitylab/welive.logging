{
	"ArtefactPublished": {
		"$schema": "http://json-schema.org/draft-04/schema#",
		"type": "object",
		"properties": {
			"providername": {
				"type": "string"
			},
			"pilot": {
				"type": "string"
			},
			"companies": {
				"type": "array",
				"items": {
					"type": "string"
				}
			},
			"license": {
				"type": "string"
			},
			"ownerid": {
				"type": "string"
			},
			"artefacttype": {
				"type": "string"
			},
			"usedartefacts": {
				"type": "array",
				"items": {
					"type": "string"
				}
			},
			"artefactid": {
				"type": "string"
			},
			"artefactname": {
				"type": "string"
			}
		},
		"required": ["providername",
		"pilot",
		"companies",
		"license",
		"ownerid",
		"artefacttype",
		"artefactid",
		"artefactname"]
	},
	"ArtefactRemoved": {
		"$schema": "http://json-schema.org/draft-04/schema#",
